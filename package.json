{
  "name": "voice-worker",
  "version": "1.0.0",
  "description": "File monitoring and parsing system with pluggable parsers",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "cli": "tsx src/cli/index.ts",
    "scripts:run": "node scripts/run.js",
    "tokens:export": "node scripts/run.js tokens export-tokens",
    "tokens:import": "node scripts/run.js tokens import-tokens",
    "tokens:extract": "node scripts/run.js tokens extract-tokens",
    "tokens:build": "cd src/nuxt-web/assets/tokens && npx style-dictionary build --config style-dictionary.config.cjs",
    "tokens:sync": "npm run build && npm run tokens:import",
    "setup:penpot": "node scripts/run.js setup setup-penpot",
    "test:services": "node scripts/run.js maintenance test-service-layer"
  },
  "keywords": [
    "file-monitoring",
    "parsing",
    "queue",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/ws": "^8.18.1",
    "axios": "^1.10.0",
    "better-sqlite3": "^9.2.2",
    "bullmq": "^4.15.1",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "openai": "^5.2.0",
    "redis": "^4.6.11",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.5",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10"
  }
}
